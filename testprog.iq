

// a query
myQuery 12345 0.2 = (
    SELECT ["foo1"];
    RENAME ["foo"] TO ["baz"];
) 

/*
    Another query
*/
myQuery2 12345 0.1 = (
    SELECT ["foo"] ;
    MAX "foo" Gauss 0.1;
)

// mapping 
mappy 123 0.1 = (
    MAP "{'baz': boo + baa}" ["baz" Int 1 25];
)

// binning
binner 123 1.0 = (
    BIN [
    "myColumn" [0, 5, 10, 15],
    "myColumn2" [0.1, 0.2, 0.3]
    ];
)